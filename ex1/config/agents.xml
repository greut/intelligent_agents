<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### REACTIVE AGENTS ### -->
    <agent name="reactive-template">
        <set class-path="bin/class"/>
        <set class-name="template.ReactiveTemplate"/>
        <set discount-factor=".85"/>
    </agent>

    <agent name="reactive-random">
        <set class-path="bin/class"/>
        <set class-name="template.ReactiveRandom"/>
        <set discount-factor=".85"/>
    </agent>

    <agent name="reactive-accept">
        <set class-path="bin/class"/>
        <set class-name="template.ReactiveAccept"/>
    </agent>


</agents>
