<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### DELIBERATIVE AGENTS ### -->
    <agent name="deliberative-agent-bfs">
        <set class-path="bin/class"/>
        <set class-name="template.DeliberativeAgent"/>
        <set algorithm="BFS"/>
    </agent>

    <agent name="deliberative-agent-a-star">
        <set class-path="bin/class"/>
        <set class-name="template.DeliberativeAgent"/>
        <set algorithm="ASTAR"/>
    </agent>

    <agent name="deliberative-agent-naive">
        <set class-path="bin/class"/>
        <set class-name="template.DeliberativeNaive"/>
        <set algorithm="BFS"/>
    </agent>

</agents>
