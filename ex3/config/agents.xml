<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### CENTRALIZED AGENTS ### -->
    <agent name="centralized-yoan">
        <set class-path="bin/class"/>
        <set class-name="template.CentralizedAgent2"/>
        <set step-probability="0.4"/>
        <set iterations="10000"/>
    </agent>

    <agent name="centralized-random">
        <set class-path="bin/class"/>
        <set class-name="template.CentralizedRandom"/>
    </agent>

</agents>
