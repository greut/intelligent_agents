<?xml version="1.0" encoding="utf-8"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### CENTRALIZED AGENTS ### -->
    <agent name="centralized-greedy">
        <set class-path="bin/class"/>
        <set class-name="template.CentralizedAgent"/>
        <set init="one"/>
        <set local-search="greedy"/>
        <set iterations="10000"/>
    </agent>

    <agent name="centralized-stochastic">
        <set class-path="bin/class"/>
        <set class-name="template.CentralizedAgent"/>
        <set init="one"/>
        <set local-search="stochastic"/>
        <set iterations="10000"/>
    </agent>

    <agent name="centralized-simulated">
        <set class-path="bin/class"/>
        <set class-name="template.CentralizedAgent"/>
        <set init="one"/>
        <set local-search="simulated"/>
        <set iterations="10000"/>
    </agent>

    <agent name="centralized-random">
        <set class-path="bin/class"/>
        <set class-name="template.CentralizedRandom"/>
    </agent>

</agents>
